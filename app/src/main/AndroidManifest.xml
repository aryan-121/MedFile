<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.speedcode.speedcode">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpeedCode">
        <activity android:name=".SugarTrackerActivity" />
        <activity android:name=".tracker" />
        <activity android:name=".creators_Activity" />
        <activity android:name=".NameChanger" />
        <activity android:name=".Setting_Activity" />
        <activity android:name=".SplashSpeedCode">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>